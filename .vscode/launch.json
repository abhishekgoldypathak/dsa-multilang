{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "C++ Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/cpp/build/main",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/cpp",
            "environment": [],
            "MIMode": "lldb"
        },
        {
            "name": "Go Launch",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${fileDirname}"
        }
    ]
}
